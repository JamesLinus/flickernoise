#!/bin/sh
. ./Common

###############################################################################

ptest "dual use: min = sin+cos" <<EOF
min = sin+cos
EOF
expect <<EOF
min = (+ (sin) (cos))
EOF

#------------------------------------------------------------------------------

ptest "dual use: sin = sqr-if" <<EOF
sin = sqr-if
EOF
expect <<EOF
sin = (- (sqr) (if))
EOF

#------------------------------------------------------------------------------

ptest "dual use: if = max*int" <<EOF
if = max*int
EOF
expect <<EOF
if = (* (max) (int))
EOF

###############################################################################
